{% extends 'base.html.twig' %}

{% block title %}TrailRunningParticipant{% endblock %}

{% block body %}
    <h1>TrailRunningParticipant</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ trail_running_participant.id }}</td>
            </tr>
            <tr>
                <th>Time</th>
                <td>{{ trail_running_participant.time ? trail_running_participant.time|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Dorsal</th>
                <td>{{ trail_running_participant.dorsal }}</td>
            </tr>
            <tr>
                <th>Banned</th>
                <td>{{ trail_running_participant.banned ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_trail_running_participant_index') }}">back to list</a>

    <a href="{{ path('app_trail_running_participant_edit', {'id': trail_running_participant.id}) }}">edit</a>

    {{ include('trail_running_participant/_delete_form.html.twig') }}
{% endblock %}
